<p id="notice"><%= notice %></p>

<h1>First Forms</h1>

<table>
  <thead>
    <tr>
      <th>Feld1</th>
      <th>Feld2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_forms.each do |first_form| %>
      <tr>
        <td><%= first_form.feld1 %></td>
        <td><%= first_form.feld2 %></td>
        <td><%= link_to 'Show', first_form %></td>
        <td><%= link_to 'Edit', edit_first_form_path(first_form) %></td>
        <td><%= link_to 'Destroy', first_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First Form', new_first_form_path %>
