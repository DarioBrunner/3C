<p id="notice"><%= notice %></p>

<h1>Second Forms</h1>

<table>
  <thead>
    <tr>
      <th>Feld1</th>
      <th>Feld2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_forms.each do |second_form| %>
      <tr>
        <td><%= second_form.feld1 %></td>
        <td><%= second_form.feld2 %></td>
        <td><%= link_to 'Show', second_form %></td>
        <td><%= link_to 'Edit', edit_second_form_path(second_form) %></td>
        <td><%= link_to 'Destroy', second_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second Form', new_second_form_path %>
