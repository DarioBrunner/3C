<p id="notice"><%= notice %></p>

<h1>Dataprotections</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dataprotections.each do |dataprotection| %>
      <tr>
        <td><%= dataprotection.content %></td>
        <%if current_user.superuser?%>
        <td><%= link_to 'Show', dataprotection %></td>
        <td><%= link_to 'Edit', edit_dataprotection_path(dataprotection) %></td>
        <td><%= link_to 'Destroy', dataprotection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%if current_user.superuser?%>
  <%= link_to 'New Faq', new_faq_path %>
<%end%>
